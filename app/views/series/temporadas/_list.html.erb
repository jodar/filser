<table class="table table-striped" id="temporada">
    <thead>
      <tr>
        <th>Title</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @temporadas.each do |temporada| %>
        <tr>
          <td><%= temporada.title %></td>
           <td><%= link_to 'Show', series_temporada_path(temporada.series, temporada) %></td>
          <td><%= link_to 'Edit', edit_series_temporada_path(temporada.series, temporada) %></td>
          <td><%= link_to 'Destroy', series_temporada_path(temporada.series, temporada), method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>